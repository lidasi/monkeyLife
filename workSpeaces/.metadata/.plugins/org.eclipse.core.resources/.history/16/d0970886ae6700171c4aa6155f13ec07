var infoPage = new function() {
	
}

infoPage.init = function(page) {
	if (page == "1") {
		alert("猿生活");
	} else if (page == "2") {
		alert("EnglishStudy")
	} else if (page == "3") {
		alert("日本語の勉強")
	} else if (page == "4") {
		 alert("日常生活")
	} else if (page == "5") {
		alert("感情写照")
	} else if (page == "6") {
		alert("脉络抒写")
	} else {
		alert("记忆珍藏")
	}
}